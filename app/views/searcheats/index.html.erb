<p id="notice"><%= notice %></p>

<h1>Listing Searcheats</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Price</th>
      <th>Location</th>
      <th>People</th>
      <th>Kind</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @searcheats.each do |searcheat| %>
      <tr>
        <td><%= searcheat.name %></td>
        <td><%= searcheat.price %></td>
        <td><%= searcheat.location %></td>
        <td><%= searcheat.people %></td>
        <td><%= searcheat.kind %></td>
        <td><%= link_to 'Show', searcheat %></td>
        <td><%= link_to 'Edit', edit_searcheat_path(searcheat) %></td>
        <td><%= link_to 'Destroy', searcheat, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Searcheat', new_searcheat_path %>
